{
  "name": "wondrous-ink",
  "private": true,
  "dependencies": {
    "jquery": "~2.*",
    "jquery-placeholder": "~2.*",
    "hammerjs": "~2.*",
    "jquery-hammerjs": "~2.0.0",
    "fastclick": "~1.0.6",
    "modernizr": "~2.8.3",
    "autosize": "~3.0.8",
    "detectizr": "~2.0.0",
    "fancybox": "~2.1.5",
    "jquery-ui": "~1.11.4",
    "ev-emitter": "*",
    "imagesloaded": "^4.1.0",
    "jquery_lazyload": "^1.9.1"
  },
  "files": {
    "retina.js": {
      "js": {
        "cwd": "dist/",
        "paths": [
          "retina.js"
        ]
      }
    },
    "jquery-ui": {
      "css": {
        "cwd": "themes/blitzer/",
        "paths": [
          "jquery-ui.min.css",
          "theme.css"
        ],
        "dest": "css/jquery-ui/"
      },
      "images": {
        "cwd": "themes/blitzer/images",
        "paths": [
          "{,helpers/}*.{gif,png}"
        ],
        "dest": "css/images/"
      }
    },
    "modernizr": {
      "js": {
        "cwd": "",
        "paths": [
          "modernizr.js"
        ]
      }
    },
    "jquery-placeholder": {
      "js": {
        "cwd": "",
        "paths": [
          "jquery.placeholder.js"
        ]
      }
    },
    "fancybox": {
      "js": {
        "cwd": "source/",
        "paths": [
          "jquery.fancybox.pack.js"
        ]
      },
      "css": {
        "cwd": "source/",
        "paths": [
          "{,helpers/}*.css"
        ],
        "dest": "css/fancybox/"
      },
      "images": {
        "cwd": "source/",
        "paths": [
          "{,helpers/}*.{gif,png}"
        ],
        "dest": "css/fancybox/"
      },
      "other": []
    }
  },
  "devDependencies": {}
}
